    "ejs": "^3.0.1",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.5.0",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "cross-env": "^3.2.4",
    "ejs": "^3.0.1",
    "expect.js": "^0.3.1",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
